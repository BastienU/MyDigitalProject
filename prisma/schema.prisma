generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  created        DateTime @default(now())
  modified       DateTime @updatedAt
  username       String
  genre          String
  email          String   @unique
  password       String
  role           String
  termsAccepted   Boolean
}